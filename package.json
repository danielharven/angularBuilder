{
  "name": "esapp-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "local-start": "ng serve --host 0.0.0.0",
    "start": "ng serve --open",
    "hmr": "ng serve --configuration=hmr --open",
    "proxy": "ng serve --proxy-config proxy.config.json --configuration=hmr --open --host 0.0.0.0",
    "build": "ng build --configuration production --build-optimizer --stats-json",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "eslint": "eslint --ext .ts src/app/components",
    "stylelint": "stylelint \"src/app/components/*.{scss,less}\"",
    "deploy": "cd .cloudflare/ && wrangler publish",
    "build-demo": "ng build --prod --build-optimizer"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "src/**/*.{ts,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts}": [
      "eslint --fix"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/core": "^13.3.1",
    "@angular-devkit/schematics": "^13.3.1",
