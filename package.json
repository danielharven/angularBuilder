    "@angular/common": "^13.3.1",
    "@angular/compiler": "^13.3.1",
    "@angular/core": "^13.3.1",
    "@angular/forms": "^13.3.1",
    "@angular/localize": "^13.3.1",
    "@angular/platform-browser": "^13.3.1",
    "@angular/platform-browser-dynamic": "^13.3.1",
    "@angular/router": "^13.3.1",
    "@ant-design/icons-angular": "^11.0.1",
    "@capacitor/core": "^3.4.3",
    "@capacitor/geolocation": "^1.3.1",
    "@ng-select/ng-select": "^8.1.1",
    "@ngrx/effects": "^13.1.0",
    "@ngrx/router-store": "^13.1.0",
    "@ngrx/store": "^13.1.0",
    "@ngx-formly/core": "^5.12.0",
    "@ngx-formly/ng-zorro-antd": "0.0.1",
    "@ngx-translate/core": "^14.0.0",
    "chalk": "^4.1.0",
    "cli-select": "^1.1.2",
    "core-js": "^3.6.5",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.15",
    "ng-zorro-antd": "^13.1.1",
    "ngx-indexed-db": "^10.0.0",
    "ngx-nestable": "^0.9.4",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-progressbar": "^8.0.0",
    "qs": "^6.9.3",
    "readline-sync": "^1.4.10",
    "rxjs": "^7.5.5",
    "rxjs-compat": "^6.6.7",
    "store": "^2.0.12",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.1",
    "@angular-eslint/builder": "4.3.1",
