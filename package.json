{
  "name": "visual-builder-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "local-start": "ng serve --host 0.0.0.0",
    "start": "ng serve",
    "hmr": "ng serve --configuration=hmr --open",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "tslint": "tslint \"src/**/*.{ts}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "postinstall": "ngcc",
    "deploy": "cd .cloudflare/ && wrangler publish",
    "build-demo": "ng build --prod --build-optimizer --configuration=demo",
    "build-deploy": "yarn vb-full && yarn build-demo && yarn deploy && yarn vb-seed",
    "vb": "node ./scripts/vb.js",
    "vb-full": "REDIRECT_URL=/dashboard/alpha node ./scripts/generate.js --config=config-full.json",
    "vb-seed": "node ./scripts/generate.js --config=config-seed.json",
    "dev:ssr": "ng run visualbuilder:serve-ssr",
    "serve:ssr": "node dist/visualbuilder/server/main.js",
    "build:ssr": "ng build --prod && ng run visualbuilder:server:production",
    "prerender": "ng run visualbuilder:prerender"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "src/**/*.{ts,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts}": [
      "tslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@angular/animations": "^11.2.1",
