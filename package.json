    "@ng-select/ng-select": "^7.2.0",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@nguniversal/express-engine": "11.2.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
